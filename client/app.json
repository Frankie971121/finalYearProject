{
  "expo": {
    "name": "Rental Mobile App",
    "slug": "Property-Rental-Mobile-App",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/rental-property.png",
    "scheme": "celodappkit",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android" : {
      "package": "com.unimas.propertyrentapp",
      "versionCode": 2
    },
    "description": "Connect with other Celo devs on our discord: https://discord.gg/6yWMkgM",
    "githubUrl": "https://github.com/critesjosh/celo-dappkit"
  }
}
